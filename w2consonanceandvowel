#include<stdio.h>
int main(){
    char ch;
    printf("enter a character");
    scanf("%c",&ch);

    if((ch>='A' && ch<='Z')  || (ch>='a'  && ch<='z')){
        if(ch=='A' || ch=='E' || ch=='I' ch=='O' ch=='U' || ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u'){
            printf("the character is vowel %c");
        }
    }
    else {
        printf("the character is consonance %c");
    }
    return 0;
}